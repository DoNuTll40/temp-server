datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

generator client {
  provider = "prisma-client-js"
}

model SensorData {
  id          Int      @id @default(autoincrement())
  room_name   String // ชื่อห้อง
  temperature Float
  humidity    Float
  createdAt   DateTime @default(now())
}
